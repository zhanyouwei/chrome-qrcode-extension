{
    "manifest_version": 2,
    "name": "二维码生成器",
    "version": "1.0",
    "description": "网页URL转换成二维码插件",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon_24.png",
            "38": "images/icon48.png"
        },
        "default_title": "二维码",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}