{
    "manifest_version": 2,
    "name": "二维码生成器",
    "version": "1.2",
    "description": "网页URL生成二维码插件,快捷键 Alt+Q",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon_24.png",
            "38": "images/icon48.png"
        },
        "default_title": "二维码",
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Alt+Q",
                "mac": "Alt+Q"
            }
        }
    },
    "permissions": [
        "tabs"
    ]
}